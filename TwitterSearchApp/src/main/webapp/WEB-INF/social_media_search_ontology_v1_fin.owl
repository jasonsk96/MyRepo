        <rdfs:label xml:lang="en">Non erosive Arthritis affecting Hands/feet/wrist/ankles</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-101120 -->

