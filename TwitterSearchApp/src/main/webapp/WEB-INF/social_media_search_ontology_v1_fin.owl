        <rdfs:label xml:lang="en">Erosive Arthritis affecting Hands/feet/wrist/ankles</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-101220 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-101220">
