    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#ANA-PAT-01 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#ANA-PAT-01">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Antinuclear-Antibody-Pattern"/>
        <rdfs:comment xml:lang="en">Speckled: Associated with SLE, Sj√∂gren syndrome, scleroderma, polymyositis, rheumatoid arthritis, and mixed connective tissue disease</rdfs:comment>
        <rdfs:label xml:lang="en">Nuclear Speckled</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#ANA-PAT-02 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#ANA-PAT-02">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Antinuclear-Antibody-Pattern"/>
        <rdfs:comment xml:lang="en">Homogenous (diffuse): Associated with Systemic Lupus Erythematosus(SLE), mixed connective tissue disease, and drug-induced lupus.</rdfs:comment>
        <rdfs:label xml:lang="en">Nuclear Homogeneous</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#ANA-PAT-03 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#ANA-PAT-03">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Antinuclear-Antibody-Pattern"/>
        <rdfs:comment xml:lang="en">Associated with scleroderma and polymyositis</rdfs:comment>
        <rdfs:label xml:lang="en">Nucleolar</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#ANA-PAT-04 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#ANA-PAT-04">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Antinuclear-Antibody-Pattern"/>
        <rdfs:comment xml:lang="en">Centromere pattern (peripheral): Associated with scleroderma and CREST (Calcinosis, Raynaud syndrome, Esophogeal dysmotility, Sclerodactyly, Telangiectasia)</rdfs:comment>
        <rdfs:label xml:lang="en">Anticentromere</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#ANA-PAT-05 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#ANA-PAT-05">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Antinuclear-Antibody-Pattern"/>
        <rdfs:comment xml:lang="en">Anti-mitochondrial antibodies (AMA) are autoantibodies, consisting of immunoglobulins formed against mitochondria, primarily the mitochondria in cells of the liver.</rdfs:comment>
        <rdfs:label xml:lang="en">Cytoplasmic Antimitochondrial</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#ANA-PAT-06 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#ANA-PAT-06">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Antinuclear-Antibody-Pattern"/>
        <rdfs:label xml:lang="en">Nuclear Other</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#ANA-PAT-07 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#ANA-PAT-07">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Antinuclear-Antibody-Pattern"/>
        <rdfs:label xml:lang="en">Cytoplasmic Other</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Antinuclear-Antibody-Pattern -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Antinuclear-Antibody-Pattern">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Medical-Test-Outcome"/>
        <rdfs:comment xml:lang="en">Patterns of cellular fluorescence.

Positive results on IFA will include a description of the particular type of fluorescent pattern seen.

Indirect fluorescent antibody (IFA) is one out of two types of tests that are commonly performed to detect and measure ANA.

This test often follows a positive ANA test (which means that autoantibodies are present).</rdfs:comment>
        <rdfs:label xml:lang="en">ANTINUCLEAR ANTIBODY PATTERN</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BIOPSY-10 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BIOPSY-10">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Biopsy-Test"/>
        <rdfs:label xml:lang="en">Biopsy Condition Exam</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BIOPSY-11 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BIOPSY-11">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BIOPSY-10"/>
        <aka>Germinal Centers</aka>
        <med-Test-Outcome>Boolean (Yes/No)</med-Test-Outcome>
        <rdfs:comment xml:lang="en">Reported evidence of Germinal Centers.

Presence of Germinal centers, that is, sites within secondary lymphoid organs ‚Äì lymph nodes and the spleen where mature B cells proliferate, differentiate, and mutate their antibody genes, and switch the class of their antibodies during a normal immune response to an infection.</rdfs:comment>
        <rdfs:label xml:lang="en">Germinal Centers [check]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BIOPSY-12 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BIOPSY-12">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BIOPSY-10"/>
        <aka>Lymphoepithelial lesion, Myoepithelial sialadenitis, MESA</aka>
        <med-Test-Outcome>Boolean (Yes/No)</med-Test-Outcome>
        <rdfs:comment xml:lang="en">Reported evidence of Lymphoepithelial Lesions.

Lymphoepithelial lesion refers to a discrete abnormality that consists of lymphoid cells and epithelium, which may or may not be benign.

https://en.wikipedia.org/wiki/Lymphoepithelial_lesion

Myoepithelial sialadenitis (MESA) is the reactive salivary gland lymphoid infiltrate that occurs in patients with Sj√∂gren&apos;s syndrome.</rdfs:comment>
        <rdfs:label xml:lang="en">Myoepithelial sialadenitis [check]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BIOPSY-13 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BIOPSY-13">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BIOPSY-10"/>
        <aka>Lymphoma</aka>
        <med-Test-Outcome>Boolean (Yes/No)</med-Test-Outcome>
        <rdfs:comment xml:lang="en">A biopsy is usually the only way to confirm lymphoma.</rdfs:comment>
        <rdfs:label xml:lang="en">Lymphoma [check]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BIOPSY-14 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BIOPSY-14">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BIOPSY-10"/>
        <aka>Fibrosis, AdiposeTissue, Atrophy</aka>
        <med-Test-Outcome>Boolean (Yes/No)</med-Test-Outcome>
        <rdfs:comment xml:lang="en">Fibrosis is the formation of excess fibrous connective tissue in an organ or tissue in a reparative or reactive process.

In biology, adipose tissue, body fat, or simply fat is a loose connective tissue composed mostly of adipocytes.

Atrophy is the partial or complete wasting away of a part of the body.</rdfs:comment>
        <rdfs:label xml:lang="en">Fibrosis / AdiposeTissue / Atrophy [check]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BIOPSY-20 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BIOPSY-20">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Biopsy-Test"/>
        <rdfs:label xml:lang="en">Biopsy Score</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BIOPSY-21 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BIOPSY-21">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BIOPSY-20"/>
        <aka>Greenspan Score, Greenspan Focus Score, Focus Score, FS</aka>
        <med-Test-Outcome>Number and/or Assessment</med-Test-Outcome>
        <rdfs:comment xml:lang="en">Greenspan and Daniels defined the focus score (FS) as the number of inflammatory infiltrates of at least 50 cells present in 4 mm^2 of gland surface unit.

A real number in range [0-12]</rdfs:comment>
        <rdfs:label xml:lang="en">Greenspan Focus Score</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BIOPSY-22 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BIOPSY-22">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BIOPSY-20"/>
        <aka>Chisholm-Mason</aka>
        <med-Test-Outcome>Number and/or Assessment</med-Test-Outcome>
        <rdfs:comment xml:lang="en">The Chisholm-Mason classification is based on assessing 4 mm^2 area of salivary gland tissue and depends on the abundance and aggregation of lymphocytes.

An integer number in range [0, 4]</rdfs:comment>
        <rdfs:label xml:lang="en">Chisholm-Mason Score</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BIOPSY-30 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BIOPSY-30">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Biopsy-Test"/>
        <rdfs:label xml:lang="en">Tissue Biomarker</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BIOPSY-31 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BIOPSY-31">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BIOPSY-30"/>
        <aka>miRNA200b-5p</aka>
        <med-Test-Normal-Range-Notes>Normal: &gt; 0.4156, Low: &lt; 0.4156</med-Test-Normal-Range-Notes>
        <med-Test-Outcome>Amount and/or Assessment</med-Test-Outcome>
        <med-Test-Outcome-Unit>pg/mL</med-Test-Outcome-Unit>
        <rdfs:comment xml:lang="en">It is a type of miRNA usually measured as a biomarker  in the salivary gland tissue specimen.</rdfs:comment>
        <rdfs:label xml:lang="en">miRNA200b-5p</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BIOPSY-32 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BIOPSY-32">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BIOPSY-30"/>
        <aka>Interferon signature Type I, IFN signature Type I</aka>
        <med-Test-Outcome>Boolean (Yes/No)</med-Test-Outcome>
        <rdfs:label xml:lang="en">Interferon (IFN) signature Type I [check] (in salivary gland biopsy)</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BIOPSY-33 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BIOPSY-33">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BIOPSY-30"/>
        <aka>Interferon signature Type II, IFN signature Type II</aka>
        <med-Test-Outcome>Boolean (Yes/No)</med-Test-Outcome>
        <rdfs:label xml:lang="en">Interferon (IFN) signature Type II [check] (in salivary gland biopsy)</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BIOPSY-34 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BIOPSY-34">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BIOPSY-30"/>
        <aka>Interferon signature Type I and II, IFN signature Type I and II</aka>
        <med-Test-Outcome>Boolean (Yes/No)</med-Test-Outcome>
        <rdfs:label xml:lang="en">Interferon (IFN) signature Type I and II [check] (in salivary gland biopsy)</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-100 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-100">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Blood-Test"/>
        <rdfs:comment xml:lang="en">The result of a lab test to measure an attribute or to determine the presence, absence or degree of a condition.</rdfs:comment>
        <rdfs:label xml:lang="en">Hematologic Test</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-110 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-110">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-100"/>
        <aka>Hemoglobin, Haemoglobin, Hb, Hgb</aka>
        <med-Test-Normal-Range-Notes>Anemia, &quot;yes&quot; if Hb &lt; 12.0 g/dL females and &lt; 13.0 g/dL males</med-Test-Normal-Range-Notes>
        <med-Test-Outcome>Amount and/or Assessment</med-Test-Outcome>
        <med-Test-Outcome-Unit>g/dL</med-Test-Outcome-Unit>
        <rdfs:comment xml:lang="en">Hemoglobin (American) or haemoglobin (British) abbreviated Hb or Hgb, is the iron-containing oxygen-transport metalloprotein in the red blood cells of all vertebrates (with the exception of the fish family Channichthyidae) as well as the tissues of some invertebrates.</rdfs:comment>
        <rdfs:label xml:lang="en">Hemoglobin [Mass/volume]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-120 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-120">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-100"/>
        <aka>Leukocytes, Leucocytes, White Blood Cells, WBCs</aka>
        <med-Test-Normal-Range-Notes>Leukopenia, &quot;yes&quot; if &lt; 4000 /mm^3</med-Test-Normal-Range-Notes>
        <med-Test-Outcome>Amount and/or Assessment</med-Test-Outcome>
        <med-Test-Outcome-Unit>#/mm^3</med-Test-Outcome-Unit>
        <rdfs:comment xml:lang="en">White blood cells (WBCs), also called leukocytes or leucocytes, are the cells of the immune system that are involved in protecting the body against both infectious disease and foreign invaders.</rdfs:comment>
        <rdfs:label xml:lang="en">Leukocytes [#/volume]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-130 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-130">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-100"/>
        <aka>Neutrophils, Absolute Neutrophils Count, ANC</aka>
        <med-Test-Normal-Range-Notes>Neutropenia, &quot;yes&quot; if &lt; 1500 /mm^3</med-Test-Normal-Range-Notes>
        <med-Test-Outcome>Amount and/or Assessment</med-Test-Outcome>
        <med-Test-Outcome-Unit>#/mm^3</med-Test-Outcome-Unit>
        <rdfs:comment xml:lang="en">An absolute neutrophils count (ANC) in blood</rdfs:comment>
        <rdfs:label xml:lang="en">Neutrophils [#/volume]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-140 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-140">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-100"/>
        <aka>Lymphocytes</aka>
        <med-Test-Normal-Range-Notes>Lymphopenia, &quot;yes&quot; if &lt; 1000 /mm^3</med-Test-Normal-Range-Notes>
        <med-Test-Outcome>Amount and/or Assessment</med-Test-Outcome>
        <med-Test-Outcome-Unit>#/mm^3</med-Test-Outcome-Unit>
        <rdfs:comment xml:lang="en">A blood test that counts how many lymphocytes are in a person&apos;s blood.

A lymphocyte is one of the subtypes of white blood cell in a vertebrate&apos;s immune system</rdfs:comment>
        <rdfs:label xml:lang="en">Lymphocytes [#/volume]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-150 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-150">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-100"/>
        <aka>Platelets, Thrombocytes</aka>
        <med-Test-Normal-Range-Notes>Thrombocytopenia, &quot;yes&quot; if &lt; 150000 /mm^3</med-Test-Normal-Range-Notes>
        <med-Test-Outcome>Amount and/or Assessment</med-Test-Outcome>
        <med-Test-Outcome-Unit>#/mm^3</med-Test-Outcome-Unit>
        <rdfs:comment xml:lang="en">Platelets, also called thrombocytes, are a component of blood whose function (along with the coagulation factors) is to react to bleeding from blood vessel injury by clumping, thereby initiating a blood clot.</rdfs:comment>
        <rdfs:label xml:lang="en">Platelets (aka Thrombocytes)  [#/volume]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-200 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-200">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Blood-Test"/>
        <rdfs:comment xml:lang="en">The Biochemistry Profile includes a range of tests that measure the constituents of the plasma or serum component of the blood.

More information available at https://www.medichecks.com/tests/biochemistry-profile</rdfs:comment>
        <rdfs:label xml:lang="en">Biochemistry Test</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-211 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-211">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-200"/>
        <aka>Albumin</aka>
        <med-Test-Normal-Range-Notes>Low serum albumin (Reference Range &gt;= 3.5 g/dl)</med-Test-Normal-Range-Notes>
        <med-Test-Outcome>Amount and/or Assessment</med-Test-Outcome>
        <med-Test-Outcome-Unit>g/dL</med-Test-Outcome-Unit>
        <rdfs:comment xml:lang="en">The serum albumin found in human blood.</rdfs:comment>
        <rdfs:label xml:lang="en">Albumin [Mass/volume]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-212 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-212">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-200"/>
        <aka>Creatinine</aka>
        <med-Test-Outcome>Amount and/or Assessment</med-Test-Outcome>
        <med-Test-Outcome-Unit>mg/dL</med-Test-Outcome-Unit>
        <rdfs:comment xml:lang="en">Creatinine is a breakdown product of creatine phosphate in muscle, and is usually produced at a fairly constant rate by the body (depending on muscle mass).</rdfs:comment>
        <rdfs:label xml:lang="en">Creatinine [Mass/volume]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-213 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-213">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-200"/>
        <aka>Folate</aka>
        <med-Test-Outcome>Amount and/or Assessment</med-Test-Outcome>
        <med-Test-Outcome-Unit>Œºg/L</med-Test-Outcome-Unit>
        <rdfs:comment xml:lang="en">Folate, distinct forms of which are known as folic acid, folacin, and vitamin B9, is one of the B vitamins.</rdfs:comment>
        <rdfs:label xml:lang="en">Folate [Mass/volume]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-214 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-214">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-200"/>
        <aka>Homocysteine</aka>
        <med-Test-Outcome>Amount and/or Assessment</med-Test-Outcome>
        <med-Test-Outcome-Unit>Œºmol/L</med-Test-Outcome-Unit>
        <rdfs:comment xml:lang="en">Homocysteine is an amino acid produced by the body and also is in your blood.</rdfs:comment>
        <rdfs:label xml:lang="en">Homocysteine Levels [Mass/volume]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-215 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-215">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-200"/>
        <aka>Uric acid</aka>
        <med-Test-Normal-Range-Notes>Normal Value: &lt; 6 mg/dL in female, &lt; 7 mg/dL in males</med-Test-Normal-Range-Notes>
        <med-Test-Outcome>Amount and/or Assessment</med-Test-Outcome>
        <med-Test-Outcome-Unit>mg/dL</med-Test-Outcome-Unit>
        <rdfs:comment xml:lang="en">A uric acid test measures the amount of uric acid in the blood.</rdfs:comment>
        <rdfs:label xml:lang="en">Uric acid [Mass/volume]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-216 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-216">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-200"/>
        <aka>Vitamin B12, B12</aka>
        <med-Test-Outcome>Amount and/or Assessment</med-Test-Outcome>
        <med-Test-Outcome-Unit>ng/mL</med-Test-Outcome-Unit>
        <rdfs:comment xml:lang="en">A vitamin B-12 test measures the amount of B-12 in your blood</rdfs:comment>
        <rdfs:label xml:lang="en">Vitamin B12 [Mass/volume]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-221 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-221">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-200"/>
        <aka>Alkaline phosphatase, ALP</aka>
        <med-Test-Outcome>Assessment Only</med-Test-Outcome>
        <rdfs:comment xml:lang="en">An alkaline phosphatase level test (ALP test) measures the amount of alkaline phosphatase enzyme in your bloodstream.</rdfs:comment>
        <rdfs:label xml:lang="en">Alkaline phosphatase (ALP)</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-222 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-222">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-200"/>
        <aka>Alanine aminotransferase, ALT</aka>
        <med-Test-Outcome>Assessment Only</med-Test-Outcome>
        <rdfs:comment xml:lang="en">Alanine aminotransferase (ALT) is an enzyme found mostly in the cells of the liver.

The alanine aminotransferase (ALT) test is typically used to detect liver injury.</rdfs:comment>
        <rdfs:label xml:lang="en">Alanine aminotransferase (ALT)</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-223 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-223">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-200"/>
        <aka>Aspartate transaminase, AST, Aspartate aminotransferase, AspAT, ASAT, AAT</aka>
        <med-Test-Outcome>Assessment Only</med-Test-Outcome>
        <rdfs:comment xml:lang="en">Aspartate transaminase (AST) or aspartate aminotransferase, also known as AspAT/ASAT/AAT or serum glutamic oxaloacetic transaminase (SGOT), is a pyridoxal phosphate (PLP)-dependent transaminase enzyme.</rdfs:comment>
        <rdfs:label xml:lang="en">Aspartate aminotransferase (AST)</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-224 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-224">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-200"/>
        <aka>Gamma glutamyl transferase, Œ≥-GT, Gamma-glutamyltransferase, Œ≥-glutamyltransferase, GGT, gamma-GT</aka>
        <med-Test-Outcome>Assessment Only</med-Test-Outcome>
        <rdfs:comment xml:lang="en">Gamma-glutamyltransferase (also Œ≥-glutamyltransferase, GGT, gamma-GT) is a transferase (a type of enzyme) that catalyzes the transfer of gamma-glutamyl functional groups from molecules such as glutathione to an acceptor that may be an amino acid, a peptide or water (forming glutamate).</rdfs:comment>
        <rdfs:label xml:lang="en">Gamma glutamyl transferase (Œ≥-GT)</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-225 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-225">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-200"/>
        <aka>Lactate dehydrogenase, LDH</aka>
        <med-Test-Outcome>Assessment Only</med-Test-Outcome>
        <rdfs:comment xml:lang="en">The lactate dehydrogenase (LDH) test looks for signs of damage to the body&apos;s tissues.

LDH is an enzyme found in almost every cell of your body, including your blood, muscles, brain, kidneys, and pancreas.</rdfs:comment>
        <rdfs:label xml:lang="en">Lactate dehydrogenase (LDH)</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-231 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-231">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-200"/>
        <aka>Potassium</aka>
        <med-Test-Outcome>Amount and/or Assessment</med-Test-Outcome>
        <med-Test-Outcome-Unit>mEq/L</med-Test-Outcome-Unit>
        <rdfs:comment xml:lang="en">Potassium (symbol K from Latin:kalium) is a key chemical element involved in neuron function and influencing osmotic balance between cells and interstitial fluid.

Depletion in potassium levels results in deficient fluid and electrolyte balance in the body as well as various nervous and cardiac dysfunctions.</rdfs:comment>
        <rdfs:label xml:lang="en">Potassium [Moles/Volume]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-232 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-232">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-200"/>
        <aka>Sodium</aka>
        <med-Test-Outcome>Amount and/or Assessment</med-Test-Outcome>
        <med-Test-Outcome-Unit>mEq/L</med-Test-Outcome-Unit>
        <rdfs:comment xml:lang="en">Sodium (Na) is a positively charged electrolyte and a mineral.

Eighty-five percent of Na is located in the blood and lymph fluid.</rdfs:comment>
        <rdfs:label xml:lang="en">Sodium [Moles/Volume]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-233 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-233">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-200"/>
        <aka>Creatine phosphokinase, CPK</aka>
        <med-Test-Outcome>Amount and/or Assessment</med-Test-Outcome>
        <med-Test-Outcome-Unit>IU/L</med-Test-Outcome-Unit>
        <rdfs:comment xml:lang="en">Creatine phosphokinase, or CPK, is an enzyme that is present in the heart, brain and skeletal muscles.

CPK is more commonly known as creatine kinase, or CK.</rdfs:comment>
        <rdfs:label xml:lang="en">Creatine phosphokinase (CPK) Muscle Enzymes [Enzymatic activity/volume]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-234 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-234">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-200"/>
        <aka>Calcium</aka>
        <med-Test-Outcome>Amount and/or Assessment</med-Test-Outcome>
        <med-Test-Outcome-Unit>mg/dL</med-Test-Outcome-Unit>
        <rdfs:comment xml:lang="en">Calcium is the most plentiful mineral in the human body.</rdfs:comment>
        <rdfs:label xml:lang="en">Calcium [Moles/Volume]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-235 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-235">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-200"/>
        <aka>Bilirubin Total, Bilirubin</aka>
        <med-Test-Outcome>Amount and/or Assessment</med-Test-Outcome>
        <med-Test-Outcome-Unit>mg/dL</med-Test-Outcome-Unit>
        <rdfs:comment xml:lang="en">All the bilirubin in your blood together is called total bilirubin.</rdfs:comment>
        <rdfs:label xml:lang="en">Bilirubin Total [Mass/Volume]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-236 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-236">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-200"/>
        <aka>Bilirubin Indirect</aka>
        <med-Test-Outcome>Amount and/or Assessment</med-Test-Outcome>
        <med-Test-Outcome-Unit>mg/dL</med-Test-Outcome-Unit>
        <rdfs:comment xml:lang="en">Bilirubin attached by the liver to glucuronic acid, a glucose-derived acid, is called direct, or conjugated, bilirubin.

Bilirubin not attached to glucuronic acid is called indirect, or unconjugated, bilirubin.</rdfs:comment>
        <rdfs:label xml:lang="en">Bilirubin Indirect [Mass/Volume]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-300 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-300">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Blood-Test"/>
        <rdfs:comment xml:lang="en">A total serum protein test measures the total amount of protein in the blood.

More information available at https://healthyeating.sfgate.com/serum-protein-levels-11390.html</rdfs:comment>
        <rdfs:label xml:lang="en">Serum Protein Test</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-310 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-310">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-300"/>
        <rdfs:comment xml:lang="en">Laboratory test for cryoglobulins.

More information available at https://labtestsonline.org/tests/cryoglobulins</rdfs:comment>
        <rdfs:label xml:lang="en">Cryoglobulin Test</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-311 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-311">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-310"/>
        <aka>Cryoglobulins</aka>
        <med-Test-Outcome>Boolean (Yes/No)</med-Test-Outcome>
        <rdfs:comment xml:lang="en">Cryoglobulins are circulating proteins, specifically immunoglobulins (i.e., IgG, IgM, IgA or light chains), that clump together (precipitate) when they are exposed to cold and dissolve when warmed.

Cryoglobulinemia, cryoglobulinaemia, or cryoglobulinemic disease, is a medical condition in which the blood contains large amounts of cryoglobulins - proteins (mostly immunoglobulins themselves) that become insoluble at reduced temperatures.</rdfs:comment>
        <rdfs:label xml:lang="en">Cryoglobulins [presence]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-312 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-312">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-310"/>
        <aka>Cryoglobulins</aka>
        <med-Test-Normal-Range-Notes>An ID from the Cryo Type (table)</med-Test-Normal-Range-Notes>
        <med-Test-Outcome>Term (type)</med-Test-Outcome>
        <rdfs:comment xml:lang="en">If the cryoglobulin test is positive, then it will be followed with protein electrophoresis and immunofixation electrophoresis (IFE) testing to determine which type(s) of protein are present as cryoglobulins and which type of cryoglobulinemia the person has.</rdfs:comment>
        <rdfs:label xml:lang="en">Cryoglobulins Type [term]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-320 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-320">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-300"/>
        <rdfs:comment xml:lang="en">This test checks the amount of certain antibodies called immunoglobulins in your body.</rdfs:comment>
        <rdfs:label xml:lang="en">Immunoglobulin Test</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-321 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-321">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-320"/>
        <aka>Immunoglobulins</aka>
        <med-Test-Outcome>Amount and/or Assessment</med-Test-Outcome>
        <med-Test-Outcome-Unit>mg/dL</med-Test-Outcome-Unit>
        <rdfs:comment xml:lang="en">Serum levels of immunoglobulins (IgG, IgA, IgM)</rdfs:comment>
        <rdfs:label xml:lang="en">Immunoglobulins [Mass/volume] (in serum)</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-322 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-322">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-320"/>
        <aka>Immunoglobulin A, IgA</aka>
        <med-Test-Outcome>Amount and/or Assessment</med-Test-Outcome>
        <med-Test-Outcome-Unit>g/L</med-Test-Outcome-Unit>
        <rdfs:comment xml:lang="en">Immunoglobulin A (IgA, also referred to as sIgA in its secretory form) is an antibody that plays a crucial role in the immune function of mucous membranes. 

The amount of IgA produced in association with mucosal membranes is greater than all other types of antibody combined</rdfs:comment>
        <rdfs:label xml:lang="en">Immunoglobulin A (IgA) [Mass/volume]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-323 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-323">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-320"/>
        <aka>Immunoglobulin G, IgG</aka>
        <med-Test-Outcome>Amount and/or Assessment</med-Test-Outcome>
        <med-Test-Outcome-Unit>g/L</med-Test-Outcome-Unit>
        <rdfs:comment xml:lang="en">Immunoglobulin G (IgG) is a type of antibody. Representing approximately 75% of serum antibodies in humans, IgG is the most common type of antibody found in blood circulation.</rdfs:comment>
        <rdfs:label xml:lang="en">Immunoglobulin G (IgG) [Mass/volume]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-324 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-324">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-320"/>
        <aka>Immunoglobulin G4, IgG4</aka>
        <med-Test-Outcome>Assessment Only</med-Test-Outcome>
        <rdfs:comment xml:lang="en">Immunoglobulin G subgroup 4 (IgG4) levels</rdfs:comment>
        <rdfs:label xml:lang="en">Immunoglobulin G4 (IgG4)</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-325 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-325">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-320"/>
        <aka>Immunoglobulin M, IgM</aka>
        <med-Test-Outcome>Amount and/or Assessment</med-Test-Outcome>
        <med-Test-Outcome-Unit>g/L</med-Test-Outcome-Unit>
        <rdfs:comment xml:lang="en">Immunoglobulin M (IgM) is one of several forms of antibody that are produced by vertebrates. 

IgM is the largest antibody, and it is the first antibody to appear in the response to initial exposure to an antigen.</rdfs:comment>
        <rdfs:label xml:lang="en">Immunoglobulin M (IgM) [Mass/volume]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-326 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-326">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-320"/>
        <aka>Kappa Free light chains</aka>
        <med-Test-Outcome>Amount and/or Assessment</med-Test-Outcome>
        <med-Test-Outcome-Unit>mg/L</med-Test-Outcome-Unit>
        <rdfs:comment xml:lang="en">Light chains are proteins produced by immune cells called plasma cells. 

Also called kappa and lambda light chains, they link together with other proteins (heavy chains) to form immunoglobulins (also known as antibodies) that target and neutralize specific threats to the body such as bacteria and viruses. 

Plasma cells normally produce some excess light chains that do not combine with heavy chains and instead enter the bloodstream....</rdfs:comment>
        <rdfs:label xml:lang="en">Kappa Free light chains [Mass/volume] (in Serum)</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-327 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-327">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-320"/>
        <aka>Lambda Free light chains</aka>
        <med-Test-Outcome>Amount and/or Assessment</med-Test-Outcome>
        <med-Test-Outcome-Unit>mg/L</med-Test-Outcome-Unit>
        <rdfs:comment xml:lang="en">Light chains are proteins produced by immune cells called plasma cells. 

Also called kappa and lambda light chains, they link together with other proteins (heavy chains) to form immunoglobulins (also known as antibodies) that target and neutralize specific threats to the body such as bacteria and viruses. 

Plasma cells normally produce some excess light chains that do not combine with heavy chains and instead enter the bloodstream....</rdfs:comment>
        <rdfs:label xml:lang="en">Lambda Free light chains [Mass/volume] (in Serum)</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-328 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-328">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-320"/>
        <aka>KAPPA / LAMBDA Free light chains ratio</aka>
        <med-Test-Outcome>Amount and/or Assessment</med-Test-Outcome>
        <med-Test-Outcome-Unit>%</med-Test-Outcome-Unit>
        <rdfs:comment xml:lang="en">There are two types of light chain in humans:

Kappa (Œ∫) chain, encoded by the immunoglobulin kappa locus on chromosome 2

Lambda (Œª) chain, encoded by the immunoglobulin lambda locus on chromosome 22</rdfs:comment>
        <rdfs:label xml:lang="en">Free light (immunoglobulin) chains KAPPA/LAMBDA ratio</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-330 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-330">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-300"/>
        <aka>Monoclonal M-component</aka>
        <med-Test-Outcome>Boolean (Yes/No)</med-Test-Outcome>
        <rdfs:comment xml:lang="en">The monoclonal proliferation of plasma cells, also known as monoclonal gammopathies, plasma cell dyscrasias, or paraproteinemias, consists of several clinicopathologic entities. 

They are all characterized by a single class of immunoglobulin (Ig) product, referred to as &quot;M-component&quot; (monoclonal component), present in serum and/or urine protein electrophoresis.</rdfs:comment>
        <rdfs:label xml:lang="en">Monoclonal M-component [presence] (in Serum)</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-340 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-340">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-300"/>
        <aka>Haptoglobin, Hp</aka>
        <med-Test-Outcome>Assessment Only</med-Test-Outcome>
        <rdfs:comment xml:lang="en">Haptoglobin (abbreviated as Hp) is the protein that in humans is encoded by the HP gene.</rdfs:comment>
        <rdfs:label xml:lang="en">Haptoglobin</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-400 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-400">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Blood-Test"/>
        <rdfs:comment xml:lang="en">Lipid profile or lipid panel is a panel of blood tests that serves as an initial screening tool for abnormalities in lipids, such as cholesterol and triglycerides.

More information available at https://oregon.providence.org/our-services/l/lipid-profile-cholesterol-and-triglycerides/</rdfs:comment>
        <rdfs:label xml:lang="en">Lipid Test</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-410 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-410">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-400"/>
        <aka>Total Cholesterol</aka>
        <med-Test-Outcome>Amount and/or Assessment</med-Test-Outcome>
        <med-Test-Outcome-Unit>mg/dL</med-Test-Outcome-Unit>
        <rdfs:comment xml:lang="en">The test for cholesterol measures total cholesterol that is carried in the blood by lipoproteins.

Cholesterol is a substance (a steroid) that is essential for life. It forms the membranes for cells in all organs and tissues in the body.</rdfs:comment>
        <rdfs:label xml:lang="en">Total cholesterol levels [Mass/volume]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-420 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-420">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-400"/>
        <aka>Low-density lipoprotein, LDL, Bad cholesterol</aka>
        <med-Test-Outcome>Amount and/or Assessment</med-Test-Outcome>
        <med-Test-Outcome-Unit>mg/dL</med-Test-Outcome-Unit>
        <rdfs:comment xml:lang="en">LDL stands for low-density lipoprotein, a type of cholesterol found in your body. 

LDL is often referred to as bad cholesterol.</rdfs:comment>
        <rdfs:label xml:lang="en">Low-density lipoprotein (LDL) cholesterol [Mass/volume]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-430 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-430">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-400"/>
        <aka>High-density lipoproteins, HDL, Good cholesterol</aka>
        <med-Test-Outcome>Amount and/or Assessment</med-Test-Outcome>
        <med-Test-Outcome-Unit>mg/dL</med-Test-Outcome-Unit>
        <rdfs:comment xml:lang="en">High-density lipoproteins (HDL) are one of the five major groups of lipoproteins

Lipoproteins are complex particles composed of multiple proteins which transport all fat molecules (lipids) around the body within the water outside cells.</rdfs:comment>
        <rdfs:label xml:lang="en">High-density lipoprotein (HDL) cholesterol [Mass/volume]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-440 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-440">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-400"/>
        <aka>Triglyceride</aka>
        <med-Test-Outcome>Amount and/or Assessment</med-Test-Outcome>
        <med-Test-Outcome-Unit>mg/dL</med-Test-Outcome-Unit>
        <rdfs:comment xml:lang="en">This test measures the amount of triglycerides in the blood.

Triglycerides are a form of fat and a major source of energy for the body.</rdfs:comment>
        <rdfs:label xml:lang="en">Triglyceride levels [Mass/volume]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-500 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-500">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Blood-Test"/>
        <rdfs:comment xml:lang="en">Autoantibodies are antibodies (immune proteins) that mistakenly target and react with a person&apos;s own tissues or organs.

Autoantibody tests are performed, along with x-rays, other imaging scans, and biopsies, to help diagnose an autoimmune disorder.

One of the most commonly ordered tests is the antinuclear antibody (ANA) test.</rdfs:comment>
        <rdfs:label xml:lang="en">Autoantibody Test</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-511 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-511">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-500"/>
        <aka>Rheumatoid factor, RF</aka>
        <med-Test-Outcome>Amount and/or Assessment</med-Test-Outcome>
        <med-Test-Outcome-Unit>IU/mL</med-Test-Outcome-Unit>
        <rdfs:comment xml:lang="en">Rheumatoid factor (RF) is the autoantibody (antibody directed against an organism&apos;s own tissues) that was first found in rheumatoid arthritis. 

It is defined as an antibody against the Fc portion of IgG (an antibody against an antibody) and different RFs can recognize different parts of the IgG-Fc.</rdfs:comment>
        <rdfs:label xml:lang="en">Rheumatoid Factor (RF) [Units/volume]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-512 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-512">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-500"/>
        <aka>Rheumatoid Factor titer, RF titer</aka>
        <med-Test-Outcome>Amount and/or Assessment</med-Test-Outcome>
        <med-Test-Outcome-Unit>IU/mL</med-Test-Outcome-Unit>
        <rdfs:comment xml:lang="en">The results of your test are reported as a titer, which is a measurement of how much your blood can be diluted before RF antibodies are undetectable.

In addition, new methods can detect the levels of rheumatoid factors in the blood (nephelometry).

Rheumatoid factor (RF) is a protein made by your immune system that can attack healthy tissue in your body.</rdfs:comment>
        <rdfs:label xml:lang="en">Rheumatoid Factor (RF) titer [Units/volume]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-521 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-521">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-500"/>
        <aka>Antinuclear antibody, ANA</aka>
        <med-Test-Outcome>Boolean (Yes/No)</med-Test-Outcome>
        <rdfs:comment xml:lang="en">The antinuclear antibody (ANA) test is used as a primary test to help evaluate a person for autoimmune disorders that affect many tissues and organs throughout the body (systemic) and is most often used as one of the tests to help diagnose systemic lupus erythematosus (SLE).</rdfs:comment>
        <rdfs:label xml:lang="en">Antinuclear Antibodies (ANA) [presence]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-522 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-522">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-500"/>
        <aka>Antinuclear Antibodies titer, ANA titer</aka>
        <med-Test-Outcome>Number and/or Assessment (The number is actually a fraction, e.g., 1/160)</med-Test-Outcome>
        <rdfs:comment xml:lang="en">Titers are expressed as ratios.</rdfs:comment>
        <rdfs:label xml:lang="en">Antinuclear Antibodies (ANA) titer</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-523 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-523">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-500"/>
        <aka>Antinuclear Antibodies pattern, ANA pattern</aka>
        <med-Test-Normal-Range-Notes>An ID from the ANA-Pattern (table)</med-Test-Normal-Range-Notes>
        <med-Test-Outcome>Term (pattern)</med-Test-Outcome>
        <rdfs:comment xml:lang="en">Patterns of cellular fluorescence</rdfs:comment>
        <rdfs:label xml:lang="en">Antinuclear Antibodies (ANA) pattern [term]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-530 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-530">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-500"/>
        <aka>Anti-Ro/SSA</aka>
        <med-Test-Outcome>Boolean (Yes/No)</med-Test-Outcome>
        <rdfs:comment xml:lang="en">Anti-SSA autoantibodies are anti-nuclear autoantibodies that are associated with many autoimmune diseases, such as systemic lupus erythematosus (SLE), SS/SLE overlap syndrome, subacute cutaneous lupus erythematosus (SCLE), neonatal lupus and primary biliary cirrhosis.</rdfs:comment>
        <rdfs:label xml:lang="en">Anti-Ro/SSA [presence]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-531 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-531">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-530"/>
        <aka>Anti-Ro52/SSA</aka>
        <med-Test-Outcome>Boolean (Yes/No)</med-Test-Outcome>
        <rdfs:label xml:lang="en">Anti-Ro52/SSA [presence]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-532 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-532">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-530"/>
        <aka>Anti-Ro60/SSA</aka>
        <med-Test-Outcome>Boolean (Yes/No)</med-Test-Outcome>
        <rdfs:label xml:lang="en">Anti-Ro60/SSA [presence]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-540 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-540">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-500"/>
        <aka>Anti-La/SSB</aka>
        <med-Test-Outcome>Boolean (Yes/No)</med-Test-Outcome>
        <rdfs:comment xml:lang="en">SS-B/La is an extractable nuclear antigen (ENA) composed of a 48-kD protein combined with RNA species.</rdfs:comment>
        <rdfs:label xml:lang="en">Anti-La/SSB [presence]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-550 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-550">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Blood-Test"/>
        <rdfs:label xml:lang="en">Other Autoantibody Test</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-551 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-551">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-550"/>
        <aka>Anti-CCP</aka>
        <med-Test-Outcome>Boolean (Yes/No)</med-Test-Outcome>
        <rdfs:comment xml:lang="en">Anti-cyclic citrullinated peptide antibody is a protein produced as part of the process that leads to joint inflammation in rheumatoid arthritis (RA).

The anti CCP antibody test can help doctors diagnose RA and determine how severe it is likely to be.</rdfs:comment>
        <rdfs:label xml:lang="en">Anti-CCP antibodies [Presence]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-552 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-552">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-550"/>
        <aka>Anti-dsDNA,  anti-double stranded DNA</aka>
        <med-Test-Outcome>Boolean (Yes/No)</med-Test-Outcome>
        <rdfs:comment xml:lang="en">The anti-double stranded DNA (anti-dsDNA) test is used to help diagnose lupus (systemic lupus erythematosus, SLE) in a person who has a positive result on a test for antinuclear antibody (ANA) and has clinical signs and symptoms that suggest lupus.</rdfs:comment>
        <rdfs:label xml:lang="en">Anti-dsDNA [Presence]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-553 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-553">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-550"/>
        <aka>Anti-Sm, Anti-Smith</aka>
        <med-Test-Outcome>Boolean (Yes/No)</med-Test-Outcome>
        <rdfs:comment xml:lang="en">Anti-Smith (Anti-Sm) antibodies are a very specific marker for systemic lupus erythematosus (SLE).</rdfs:comment>
        <rdfs:label xml:lang="en">Anti-Smith (Anti-Sm) antibodies [Presence]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-554 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-554">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-550"/>
        <aka>Anti-RNP, Anti-ribonucleoprotein</aka>
        <med-Test-Outcome>Boolean (Yes/No)</med-Test-Outcome>
        <rdfs:comment xml:lang="en">Anti-ribonucleoprotein (RNP) antibodies.</rdfs:comment>
        <rdfs:label xml:lang="en">Anti-RNP antibodies [Presence]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-555 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-555">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-550"/>
        <aka>Anti-Scl-70, anti-topoisomerase</aka>
        <med-Test-Outcome>Boolean (Yes/No)</med-Test-Outcome>
        <rdfs:comment>Anti-Scl-70 (also called anti-topoisomerase I after the type I topoisomerase target) is an anti-topoisomerase antibody-type of anti-nuclear autoantibodies.</rdfs:comment>
        <rdfs:label xml:lang="en">Anti-Scl-70 antibodies [Presence]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-556 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-556">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-550"/>
        <aka>Anti-synthetase</aka>
        <med-Test-Outcome>Boolean (Yes/No)</med-Test-Outcome>
        <rdfs:label xml:lang="en">Anti-synthetase antibodies (any) [Presence]</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-557 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-557">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#BLOOD-550"/>
        <aka>Lupus Anticoagulant, LA</aka>
        <med-Test-Outcome>Boolean (Yes/No)</med-Test-Outcome>
        <rdfs:comment xml:lang="en">Lupus anticoagulant testing is a series of tests used to detect lupus anticoagulant (LA) in the blood.

Lupus anticoagulants (LA) are autoantibodies produced by the immune system that mistakenly attack certain components of the body&apos;s own cells.</rdfs:comment>
