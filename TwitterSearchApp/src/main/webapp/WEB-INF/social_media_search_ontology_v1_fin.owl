        <rdfs:label xml:lang="en">Non erosive Arthritis affecting other joints</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-101200 -->

