        <rdfs:comment xml:lang="en">Erosive arthritis has a broad differential, including erosive osteoarthritis, rheumatoid arthritis, etc.</rdfs:comment>
        <rdfs:label xml:lang="en">Erosive Arthritis</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-101210 -->

