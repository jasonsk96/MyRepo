    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-084 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-084">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-080"/>
        <acronym>Dyspareunia-VAS</acronym>
        <quest-Outcome>Number and/or Assessment</quest-Outcome>
        <quest-Score-Normal-Range>0</quest-Score-Normal-Range>
        <rdfs:comment xml:lang="en">Dyspareunia is being measured using a 10 cm visual analogue scale (VAS), where 0 indicated the absence of pain and 10 indicated worst possible pain.</rdfs:comment>
        <rdfs:label xml:lang="en">Dyspareunia Visual Analogue Scale (VAS) score</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-085 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-085">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-080"/>
        <acronym>Fatigue-VAS</acronym>
        <quest-Outcome>Number and/or Assessment</quest-Outcome>
        <quest-Score-Normal-Range>0</quest-Score-Normal-Range>
        <rdfs:label xml:lang="en">Fatigue Visual Analogue Scale (VAS) score</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-086 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-086">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-080"/>
        <acronym>EQ-VAS</acronym>
        <quest-Outcome>Number and/or Assessment</quest-Outcome>
        <quest-Score-Normal-Range>100</quest-Score-Normal-Range>
        <rdfs:label xml:lang="en">EuroQol Group (EQ) Visual Analogue Scale (VAS) score</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-090 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-090">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Questionnaire-Score"/>
        <acronym>EPQ</acronym>
        <quest-Outcome>Assessment Only</quest-Outcome>
        <rdfs:comment xml:lang="en">Eysenck Personality Questionnaire (EPQ) is a questionnaire to assess the personality traits of a person.

Eysenck initially conceptualized personality as two biologically-based independent dimensions of temperament, E and N, measured on a continuum, but then extending this to include a third, P as well as a fourth L.

Although the first 3 scales were predicted upon a biologically based theory of personality, the fourth scale has not been theoretically specified to the same extent, but it was considered to be conceptually strong to the extent that it would demonstrate the same degree of measurement similarity across cultures.</rdfs:comment>
        <rdfs:label xml:lang="en">Eysenck Personality Questionnaire (EPQ) score</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-091 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-091">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-090"/>
        <acronym>EPQ-E</acronym>
        <quest-Outcome>Number and/or Assessment</quest-Outcome>
        <quest-Score-Normal-Range>=/&lt; 9</quest-Score-Normal-Range>
        <rdfs:comment xml:lang="en">Extraversion is characterized by being outgoing, talkative, high on positive affect (feeling good), and in need of external stimulation.</rdfs:comment>
        <rdfs:label xml:lang="en">Eysenck Personality Questionnaire (EPQ) - Extraversion/Introversion value</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-092 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-092">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-090"/>
        <acronym>EPQ-N</acronym>
        <quest-Outcome>Number and/or Assessment</quest-Outcome>
        <quest-Score-Normal-Range>=/&lt; 12</quest-Score-Normal-Range>
        <rdfs:comment xml:lang="en">Neuroticism or emotionality is characterized by high levels of negative affect such as depression and anxiety.</rdfs:comment>
        <rdfs:label xml:lang="en">Eysenck Personality Questionnaire (EPQ) - Neuroticism/Stability value</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-093 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-093">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-090"/>
        <acronym>EPQ-P</acronym>
        <quest-Outcome>Number and/or Assessment</quest-Outcome>
        <quest-Score-Normal-Range>=/&lt; 2</quest-Score-Normal-Range>
        <rdfs:comment xml:lang="en">Psychoticism is associated not only with the liability to have a psychotic episode (or break with reality), but also with aggression.</rdfs:comment>
        <rdfs:label xml:lang="en">Eysenck Personality Questionnaire (EPQ) - Psychoticism/Socialisation value</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-094 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-094">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-090"/>
        <acronym>EPQ-L</acronym>
        <quest-Outcome>Number and/or Assessment</quest-Outcome>
        <quest-Score-Normal-Range>=/&lt; 12</quest-Score-Normal-Range>
        <rdfs:label xml:lang="en">Eysenck Personality Questionnaire (EPQ) - Lie/Social Desirability value</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-100 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-100">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Questionnaire-Score"/>
        <acronym>STAI</acronym>
        <quest-Outcome>Assessment Only</quest-Outcome>
        <rdfs:comment xml:lang="en">The State Trait Anxiety Inventory is a test/questionnaire given to adults that shows how strong a person&apos;s feelings of anxiety are. 

The State-Trait Anxiety Inventory (STAI) is a psychological inventory based on a 4-point Likert scale and consists of 40 questions on a self-report basis. 

The STAI measures two types of anxiety: state anxiety, or anxiety about an event (STAI-S), and trait anxiety, or anxiety level as a personal characteristic (STAI-T).

Higher scores are positively correlated with higher levels of Anxiety.

The STAI includes a 40 question response taking approximately 10-20 minutes for completion and the test is given in tens of different languages worldwide. 

This test is split into the S-Anxiety scale and the T-Anxiety scale, each having 20 items. 

These tests are answered on the basis of a 1-4 scale, with the focused areas including: worry, tension, apprehension, and nervousness. 

The current edition is Form Y (STAI Form Y).</rdfs:comment>
        <rdfs:label xml:lang="en">State-Trait Anxiety Inventory (STAI) score</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-101 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-101">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-100"/>
        <acronym>STAI-S</acronym>
        <quest-Outcome>Number and/or Assessment</quest-Outcome>
        <quest-Score-Normal-Range>=/&lt; 35</quest-Score-Normal-Range>
        <rdfs:comment xml:lang="en">S-Anxiety scale (part of STAI) consits of 20 items.</rdfs:comment>
        <rdfs:label xml:lang="en">State-Trait Anxiety Inventory (STAI) - State Anxiety score</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-102 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-102">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-100"/>
        <acronym>STAI-T</acronym>
        <quest-Outcome>Number and/or Assessment</quest-Outcome>
        <quest-Score-Normal-Range>=/&lt; 35</quest-Score-Normal-Range>
        <rdfs:comment xml:lang="en">T-Anxiety scale (part of STAI) consits of 20 items.</rdfs:comment>
        <rdfs:label xml:lang="en">State-Trait Anxiety Inventory (STAI) - Trait Anxiety score</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-110 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-110">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Questionnaire-Score"/>
        <acronym>HADS</acronym>
        <quest-Outcome>Assessment Only</quest-Outcome>
        <quest-Score-Normal-Range>0</quest-Score-Normal-Range>
        <rdfs:comment xml:lang="en">Hospital Anxiety and Depression Scale (HADS) is a questionnaire that is commonly used by the doctors to determine the levels of anxiety and depression that a patient is experiencing.

Note: Each item on the questionnaire is scored from 0-3 and this means that a person can score between 0 and 21 for either anxiety or depression.</rdfs:comment>
        <rdfs:label xml:lang="en">Hospital anxiety and depression scale (HADS) score</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-111 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-111">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-110"/>
        <acronym>HADS-A</acronym>
        <quest-Outcome>Number and/or Assessment</quest-Outcome>
        <quest-Score-Normal-Range rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</quest-Score-Normal-Range>
        <rdfs:comment xml:lang="en">Total score based on responses provided in questions related to anxiety. 

The items on the questionnaire that relate to anxiety are
 - I feel tense or wound up
 - I get a sort of frightened feeling as if something bad is about to happen
 - Worrying thoughts go through my mind
 - I can sit at ease and feel relaxed
 - I get a sort of frightened feeling like butterflies in the stomach
 - I feel restless and have to be on the move
 - I get sudden feelings of panic

Each item on the questionnaire is scored from 0-3 and this means that a person can score between 0 and 21.</rdfs:comment>
        <rdfs:label xml:lang="en">HADS - Anxiety Score</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-112 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-112">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-110"/>
        <acronym>HADS-D</acronym>
        <quest-Outcome>Number and/or Assessment</quest-Outcome>
        <quest-Score-Normal-Range rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</quest-Score-Normal-Range>
        <rdfs:comment xml:lang="en">Total score based on responses provided in questions related to depression. 

The items that relate to depression are:
 - I still enjoy the things I used to enjoy
 - I can laugh and see the funny side of things
 - I feel cheerful
 - I feel as if I am slowed down
 - I have lost interest in my appearance
 - I look forward with enjoyment to things
 - I can enjoy a good book or radio or TV program

Each item on the questionnaire is scored from 0-3 and this means that a person can score between 0 and 21.</rdfs:comment>
        <rdfs:label xml:lang="en">HADS - Depression Score</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-120 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-120">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Questionnaire-Score"/>
        <acronym>AIS</acronym>
        <quest-Outcome>Number and/or Assessment</quest-Outcome>
        <quest-Score-Normal-Range>=/&lt; 6</quest-Score-Normal-Range>
        <rdfs:comment xml:lang="en">It is measured by assessing eight factors (as tabulated below) amongst which first five factors are related to nocturnal sleep and last three factors are related to daytime dysfunction. 

These are rated on a 0‚Äì3 scale and the sleep is finally evaluated from the cumulative score of all factors and reported as an individual&apos;s sleep outcome. 

A cut-off score of ‚â• 6 on the AIS is used to establish the diagnosis of insomnia.</rdfs:comment>
        <rdfs:label xml:lang="en">Athens Insomnia Scale (AIS) score</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-130 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-130">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Questionnaire-Score"/>
        <acronym>ZSDS</acronym>
        <quest-Outcome>Number and/or Assessment</quest-Outcome>
        <quest-Score-Normal-Range>=/&lt; 40</quest-Score-Normal-Range>
        <rdfs:comment xml:lang="en">The Zung Self-Rating Depression Scale is a short self-administered survey to quantify the depressed status of a patient. 

There are 20 items on the scale that rate the affective, psychological and somatic symptoms associated with depression.

There are ten positively worded and ten negatively worded questions. Each question is scored on a scale of 1 through 4 (based on these replies: &quot;a little of the time&quot;, &quot;some of the time&quot;, &quot;good part of the time&quot;, &quot;most of the time&quot;).

Scores on the test range from 20 through 80.</rdfs:comment>
        <rdfs:label xml:lang="en">Zung Self-Rating Depression Scale (ZSDS) score</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-140 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-140">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Questionnaire-Score"/>
        <acronym>PROFAD</acronym>
        <quest-Outcome>Number and/or Assessment</quest-Outcome>
        <quest-Score-Normal-Range>0</quest-Score-Normal-Range>
        <rdfs:comment xml:lang="en">The Value is a Number in range: 0 - 28</rdfs:comment>
        <rdfs:label xml:lang="en">Profile of Fatigue and Discomfort (PROFAD) score</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-150 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-150">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Questionnaire-Score"/>
        <acronym>SSI</acronym>
        <quest-Outcome>Number and/or Assessment</quest-Outcome>
        <quest-Score-Normal-Range rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</quest-Score-Normal-Range>
        <rdfs:comment xml:lang="en">The Value is a Number in range: 0 - 28</rdfs:comment>
        <rdfs:label xml:lang="en">Sicca Symptoms Inventory (SSI) score</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-160 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-160">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Questionnaire-Score"/>
        <acronym>PROFAD-SSI-SF</acronym>
        <quest-Outcome>Number and/or Assessment</quest-Outcome>
        <quest-Score-Normal-Range>0</quest-Score-Normal-Range>
        <rdfs:comment xml:lang="en">The Profile of Fatigue and Discomfort Sicca Symptoms Inventory (PROFAD-SSI) was developed as a measurement tool for patients with Primary Sjogren&apos;s Syndrome (pSS) and other rheumatic disorders. 

The Short Form (SF) contains 19 items.

It documents patient-reported symptoms of ocular, oral, cutaneous and vaginal dryness.

The Value is a Number in range: 0 - 28</rdfs:comment>
        <rdfs:label xml:lang="en">Profile of Fatigue and Discomfort - Sicca Symptoms Inventory - Short Form (PROFAD-SSI-SF) score</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Questionnaire-Element -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Questionnaire-Element">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Questionnaires"/>
        <rdfs:comment xml:lang="en">An item from a specific Questionnaire.</rdfs:comment>
        <rdfs:label xml:lang="en">QUESTIONNAIRE ITEM</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Questionnaire-Score -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Questionnaire-Score">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Questionnaires"/>
        <rdfs:comment xml:lang="en">A number calculated based on the answers provided in the questions of a specific questionnaire.</rdfs:comment>
        <rdfs:label xml:lang="en">QUESTIONNAIRE SCORE</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Questionnaires -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Questionnaires">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Exams"/>
        <rdfs:comment xml:lang="en">Controlled set of Terms used for the storage of data about Questionnaires and Scores.</rdfs:comment>
        <rdfs:label xml:lang="en">Questionnaires</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#REL-DEG-01 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#REL-DEG-01">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Relative-Degree"/>
        <rdfs:comment xml:lang="en">A first-degree relative is one&apos;s offspring, sibling or parent.</rdfs:comment>
        <rdfs:label xml:lang="en">First Degree</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#REL-DEG-02 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#REL-DEG-02">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Relative-Degree"/>
        <rdfs:comment xml:lang="en">A second-degree relative (SDR) is someone who shares 25% of a person&apos;s genes. 

It includes uncles, aunts, nephews, nieces, grandparents, grandchildren, half-siblings, and double cousins.</rdfs:comment>
        <rdfs:label xml:lang="en">Second Degree</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#REL-DEG-03 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#REL-DEG-03">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Relative-Degree"/>
        <rdfs:comment xml:lang="en">Third-degree relatives constitute a category of family members that constitutes a segment of the extended family and includes first cousins, great grandparents and great grandchildren.</rdfs:comment>
        <rdfs:label xml:lang="en">Third Degree</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#REL-DEG-99 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#REL-DEG-99">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Relative-Degree"/>
        <rdfs:comment xml:lang="en">A Relative Degree that do not belong to the other three categories.</rdfs:comment>
        <rdfs:label xml:lang="en">Other Relative Degree</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Relative-Degree -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Relative-Degree">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Conditions"/>
        <rdfs:comment xml:lang="en">Family relations can be represented concretely (mother, brother, grandfather) or abstractly by degrees of relationship (kinship distance)

A relationship may be relative (e.g. a father in relation to a child) or reflect an absolute (e.g. the difference between a mother and a childless woman). 

Note: Degrees of relationship are not identical to heirship or legal succession.</rdfs:comment>
        <rdfs:label xml:lang="en">RELATIVE DEGREE</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SAL-BIO-10 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SAL-BIO-10">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Salivary-Gland-Biopsy"/>
        <rdfs:comment xml:lang="en">The (major) salivary glands are found in and around the mouth, throat and face. The largest or major salivary glands are the parotid, submandibular, and sublingual glands.</rdfs:comment>
        <rdfs:label xml:lang="en">Major Salivary Gland Biopsy</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SAL-BIO-11 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SAL-BIO-11">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SAL-BIO-10"/>
        <rdfs:comment xml:lang="en">The parotid gland is a major salivary gland in many animals. 

In humans, the two parotid glands are present on either side of the mouth and in front of both ears. 

They are the largest of the salivary glands.</rdfs:comment>
        <rdfs:label xml:lang="en">Parotid Gland Biopsy</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SAL-BIO-12 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SAL-BIO-12">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SAL-BIO-10"/>
        <rdfs:comment xml:lang="en">The paired submandibular glands (historically known as submaxillary glands) are major salivary glands located beneath the floor of the mouth.</rdfs:comment>
        <rdfs:label xml:lang="en">Submandibular Gland Biopsy</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SAL-BIO-13 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SAL-BIO-13">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SAL-BIO-10"/>
        <rdfs:comment xml:lang="en">The paired sublingual glands are major salivary glands in the mouth. 

They are the smallest, most diffuse, and the only unencapsulated major salivary glands.</rdfs:comment>
        <rdfs:label xml:lang="en">Sublingual Gland Biopsy</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SAL-BIO-20 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SAL-BIO-20">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Salivary-Gland-Biopsy"/>
        <rdfs:comment xml:lang="en">There are 800 to 1,000 minor salivary glands located throughout the oral cavity within the submucosa of the oral mucosa in the tissue of the buccal, labial, and lingual mucosa, the soft palate, the lateral parts of the hard palate, and the floor of the mouth or between muscle fibers of the tongue.</rdfs:comment>
        <rdfs:label xml:lang="en">Minor Salivary Gland Biopsy</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SAL-BIO-21 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SAL-BIO-21">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SAL-BIO-20"/>
        <rdfs:comment xml:lang="en">Labial Biopsy (i.e., Lips Biopsy) is based on Glands extracted from the Lower Lip

Biopsy of the labial minor salivary glands (&quot;lip biopsy&quot;) is often used as part of the diagnostic workup for suspected Sjogren&apos;s syndrome, and is comparable in diagnostic accuracy to open parotid biopsy.

More information available at https://www.sciencedirect.com/science/article/pii/S1043181008000754

The labial glands are minor salivary glands situated between the mucous membrane and the Orbicularis oris around the orifice of the mouth.

More information available at https://en.wikipedia.org/wiki/Labial_glands</rdfs:comment>
        <rdfs:label xml:lang="en">Labial Gland (lip) Biopsy</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SEX-01 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SEX-01">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Sex"/>
        <acronym>M</acronym>
        <rdfs:comment xml:lang="en">A male organism is the physiological sex that produces sperm.</rdfs:comment>
        <rdfs:label xml:lang="en">Male</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SEX-02 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SEX-02">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Sex"/>
        <acronym>F</acronym>
        <rdfs:comment xml:lang="en">Female is the sex of an organism, or a part of an organism, that produces non-mobile ova (egg cells).</rdfs:comment>
        <rdfs:label xml:lang="en">Female</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SEX-99 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SEX-99">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Sex"/>
        <acronym>O</acronym>
        <rdfs:comment xml:lang="en">The sex of a person could not be uniquely defined as male or female, such as hermaphrodite.</rdfs:comment>
        <rdfs:label xml:lang="en">Other Sex</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SMOK-STAT-01 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SMOK-STAT-01">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Tobacco-Consumption-Status"/>
        <aka>Smoker</aka>
        <rdfs:comment xml:lang="en">The person is a Current smoker. 

Someone who has smoked greater than 100 cigarettes (including hand rolled cigarettes, cigars, cigarillos etc) in their lifetime and has smoked in the last 28 days.</rdfs:comment>
        <rdfs:label xml:lang="en">Active Smoker</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SMOK-STAT-02 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SMOK-STAT-02">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Tobacco-Consumption-Status"/>
        <aka>Previous Smoker</aka>
        <rdfs:comment xml:lang="en">The person was smoking in the past (being an ex-smoker)

Ex-smoker is someone who has smoked greater than 100 cigarettes in their lifetime but has not smoked in the last 28 days.</rdfs:comment>
        <rdfs:label xml:lang="en">Ex-Smoker</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SMOK-STAT-03 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SMOK-STAT-03">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Tobacco-Consumption-Status"/>
        <aka>Non Smoker</aka>
        <rdfs:comment xml:lang="en">He/She has never smoked in his life.

Never smoker is someone who has not smoked greater than 100 cigarettes in their lifetime and does not currently smoke.</rdfs:comment>
        <rdfs:label xml:lang="en">Never Smoker</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SPEC-01 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SPEC-01">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Specialty-Department"/>
        <rdfs:comment xml:lang="en">Ophthalmology is a branch of medicine and surgery (both methods are used) that deals with the diagnosis and treatment of eye disorders.

An ophthalmologist is a specialist in Ophthalmology.

More information available at https://en.wikipedia.org/wiki/Ophthalmology</rdfs:comment>
        <rdfs:label xml:lang="en">Ophthalmologist</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SPEC-02 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SPEC-02">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Specialty-Department"/>
        <rdfs:comment xml:lang="en">Oral medicine (sometimes termed dental medicine, oral and maxillofacial medicine or stomatology) is a specialty focused on the mouth and nearby structures.

It lies at the interface between medicine and dentistry.

More information available at https://en.wikipedia.org/wiki/Oral_medicine</rdfs:comment>
        <rdfs:label xml:lang="en">Oral medicine / dentist</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SPEC-03 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SPEC-03">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Specialty-Department"/>
        <acronym>ENT</acronym>
        <aka>otolaryngology</aka>
        <rdfs:comment xml:lang="en">Otorhinolaryngology (also called otolaryngology and otolaryngology‚Äìhead and neck surgery) is a surgical subspecialty within medicine that deals with conditions of the ear, nose, and throat (ENT) and related structures of the head and neck.

Doctors who specialize in this area are called otorhinolaryngologists, otolaryngologists, ENT doctors, ENT surgeons, or head and neck surgeons

More information available at https://en.wikipedia.org/wiki/Otorhinolaryngology</rdfs:comment>
        <rdfs:label xml:lang="en">Ear, Nose, and Throat</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SPEC-04 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SPEC-04">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Specialty-Department"/>
        <rdfs:comment xml:lang="en">Neurology is a branch of medicine dealing with disorders of the nervous system.

A neurologist is a physician specializing in neurology and trained to investigate, or diagnose and treat neurological disorders.

More information available at https://en.wikipedia.org/wiki/Neurology</rdfs:comment>
        <rdfs:label xml:lang="en">Nephrologist</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SPEC-05 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SPEC-05">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Specialty-Department"/>
        <rdfs:comment xml:lang="en">Pulmonology is a medical speciality that deals with diseases involving the respiratory tract.

Pulmonology is known as chest medicine and respiratory medicine in some countries and areas.

More information available at https://en.wikipedia.org/wiki/Pulmonology</rdfs:comment>
        <rdfs:label xml:lang="en">Pulmonologist</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SPEC-06 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SPEC-06">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Specialty-Department"/>
        <rdfs:comment xml:lang="en">Hematology, also spelled haematology, is the branch of medicine concerned with the study of the cause, prognosis, treatment, and prevention of diseases related to blood.

Physicians specialized in hematology are known as hematologists or haematologists.

More information available at https://en.wikipedia.org/wiki/Hematology</rdfs:comment>
        <rdfs:label xml:lang="en">Hematologist</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SPEC-07 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SPEC-07">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Specialty-Department"/>
        <rdfs:comment xml:lang="en">Gastroenterology is the branch of medicine focused on the digestive system and its disorders.

Physicians practicing in this field are called gastroenterologists.

More information available at https://en.wikipedia.org/wiki/Gastroenterology</rdfs:comment>
        <rdfs:label xml:lang="en">Gastroenterologist</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SPEC-08 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SPEC-08">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Specialty-Department"/>
        <rdfs:comment xml:lang="en">Neurology is a branch of medicine dealing with disorders of the nervous system.

A neurologist is a physician specializing in neurology and trained to investigate, or diagnose and treat neurological disorders.

More information available at https://en.wikipedia.org/wiki/Neurology</rdfs:comment>
        <rdfs:label xml:lang="en">Neurologist</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SPEC-09 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SPEC-09">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Specialty-Department"/>
        <rdfs:comment xml:lang="en">A psychiatrist is a physician who specializes in psychiatry, the branch of medicine devoted to the diagnosis, prevention, study, and treatment of mental disorders.

More information available at https://en.wikipedia.org/wiki/Psychiatrist</rdfs:comment>
        <rdfs:label xml:lang="en">Psychiatrist</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SPEC-10 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SPEC-10">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Specialty-Department"/>
        <rdfs:comment xml:lang="en">Gynaecology or gynecology is the medical practice dealing with the health of the female reproductive systems (vagina, uterus, and ovaries) and the breasts.

More information available at https://en.wikipedia.org/wiki/Gynaecology</rdfs:comment>
        <rdfs:label xml:lang="en">Gynecologist</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SPEC-11 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SPEC-11">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Specialty-Department"/>
        <rdfs:comment xml:lang="en">Endocrinology is a branch of biology and medicine dealing with the endocrine system, its diseases, and its specific secretions known as hormones.

An endocrinologist is a physician who specializes in treating disorders of the endocrine system, such as diabetes, hyperthyroidism, and many others.

More information available at https://en.wikipedia.org/wiki/Endocrinology</rdfs:comment>
        <rdfs:label xml:lang="en">Endocrinologists</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SPEC-12 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SPEC-12">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Specialty-Department"/>
        <rdfs:comment xml:lang="en">Dermatology is the branch of medicine dealing with the skin, nails, hair and its diseases.

A dermatologist is a doctor that treats diseases, in the widest sense, and some cosmetic problems of the skin, scalp, hair, and nails.

More information available at https://en.wikipedia.org/wiki/Dermatology</rdfs:comment>
        <rdfs:label xml:lang="en">Dermatologist</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SPEC-99 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SPEC-99">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Specialty-Department"/>
        <rdfs:label xml:lang="en">Other specialist</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#STAGE-01 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#STAGE-01">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Lymphoma-Stage"/>
        <rdfs:comment xml:lang="en">Involvement of a single lymph node region (eg, cervical, axillary, inguinal, mediastinal) or lymphoid structure such as the spleen, thymus, or Waldeyer&apos;s ring.</rdfs:comment>
        <rdfs:label xml:lang="en">Lymphoma Stage I</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#STAGE-02 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#STAGE-02">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Lymphoma-Stage"/>
        <rdfs:comment xml:lang="en">Involvement of two or more lymph node regions or lymph node structures on the same side of the diaphragm. 

Hilar nodes should be considered to be &quot;lateralized&quot; and when involved on both sides, constitute stage II disease. 

For the purpose of defining the number of anatomic regions, all nodal disease within the mediastinum is considered to be a single lymph node region, and hilar involvement constitutes an additional site of involvement. 

The number of anatomic regions should be indicated by a subscript (eg, II-3).</rdfs:comment>
        <rdfs:label xml:lang="en">Lymphoma Stage II</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#STAGE-03 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#STAGE-03">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Lymphoma-Stage"/>
        <rdfs:comment xml:lang="en">Involvement of lymph node regions or lymphoid structures on both sides of the diaphragm. 

This may be subdivided stage III-1 or III-2: stage III-1 is used for patients with involvement of the spleen or splenic hilar, celiac, or portal nodes; and stage III-2 is used for patients with involvement of the paraaortic, iliac, inguinal, or mesenteric nodes.</rdfs:comment>
        <rdfs:label xml:lang="en">Lymphoma Stage III</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#STAGE-04 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#STAGE-04">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Lymphoma-Stage"/>
        <rdfs:comment xml:lang="en">Diffuse or disseminated involvement of one or more extranodal organs or tissue beyond that designated E, with or without associated lymph node involvement.</rdfs:comment>
        <rdfs:label xml:lang="en">Lymphoma Stage IV</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SYMPT-010 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SYMPT-010">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SYMPT-CAT-A"/>
        <rdfs:comment xml:lang="en">Swollen glands are a sign that your body is fighting off an infection or an illness.</rdfs:comment>
        <rdfs:label xml:lang="en">Parotid or Submandibular swelling</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SYMPT-011 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SYMPT-011">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SYMPT-010"/>
        <rdfs:comment xml:lang="en">In humans, the two parotid glands are present on either side of the mouth and in front of both ears.

Swollen glands are a sign that your body is fighting off an infection or an illness.</rdfs:comment>
        <rdfs:label xml:lang="en">Parotid Gland swelling</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SYMPT-012 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SYMPT-012">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SYMPT-010"/>
        <rdfs:comment xml:lang="en">The paired submandibular glands (historically known as submaxillary glands) are major salivary glands located beneath the floor of the mouth.

Swollen glands are a sign that your body is fighting off an infection or an illness.</rdfs:comment>
        <rdfs:label xml:lang="en">Submandibular salivary gland swelling</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SYMPT-020 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SYMPT-020">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SYMPT-CAT-A"/>
        <aka>Xerostomia</aka>
        <rdfs:comment xml:lang="en">Dry mouth, or xerostomia, refers to a condition in which the salivary glands in your mouth don&apos;t make enough saliva to keep your mouth wet.</rdfs:comment>
        <rdfs:label xml:lang="en">Dry Mouth</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SYMPT-030 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SYMPT-030">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SYMPT-CAT-A"/>
        <rdfs:comment xml:lang="en">Dry eyes is a common condition that occurs when your tears aren&apos;t able to provide adequate lubrication for your eyes</rdfs:comment>
        <rdfs:label xml:lang="en">Dry Eyes</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SYMPT-110 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SYMPT-110">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SYMPT-CAT-B"/>
        <rdfs:label xml:lang="en">Dry Nose</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SYMPT-120 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SYMPT-120">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SYMPT-CAT-B"/>
        <rdfs:comment xml:lang="en">A dry cough is a cough where no phlegm or mucus is produced (known as non-productive).</rdfs:comment>
        <rdfs:label xml:lang="en">Dry Cough</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SYMPT-130 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SYMPT-130">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SYMPT-CAT-B"/>
        <rdfs:comment xml:lang="en">Dry skin is a very common skin condition characterized by a lack of the appropriate amount of water in the most superficial layer of the skin, the epidermis.</rdfs:comment>
        <rdfs:label xml:lang="en">Dry Skin</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SYMPT-140 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SYMPT-140">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#SYMPT-CAT-B"/>
        <rdfs:comment xml:lang="en">Fever is also known as hyperthermia, pyrexia, or elevated temperature. It describes a body temperature that‚Äôs higher than normal.</rdfs:comment>
