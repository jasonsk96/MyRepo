    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#CHEM-51504 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#CHEM-51504">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#CHEM-51500"/>
        <rdfs:comment xml:lang="en">Antidiabetic agents refer to all the different types of medicine involved in the treatment of diabetes. 

More information available at https://www.drugs.com/drug-class/antidiabetic-agents.html</rdfs:comment>
        <rdfs:label xml:lang="en">Anti-diabetic Treatment</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#CHEM-60000 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#CHEM-60000">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Drug-Substance"/>
        <rdfs:comment xml:lang="en">Vaccination is the administration of antigenic material (a vaccine) to stimulate an individual&apos;s immune system to develop adaptive immunity to a pathogen.

More information available at https://en.wikipedia.org/wiki/Vaccination</rdfs:comment>
        <rdfs:label xml:lang="en">Vaccination</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#CHEM-61000 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#CHEM-61000">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#CHEM-60000"/>
        <rdfs:comment xml:lang="en">Flu vaccines cause antibodies to develop in the body about two weeks after vaccination.

These antibodies provide protection against infection with the viruses that are in the vaccine.</rdfs:comment>
        <rdfs:label xml:lang="en">Flu Vaccination</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#CHEM-62000 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#CHEM-62000">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#CHEM-60000"/>
        <rdfs:comment xml:lang="en">Vaccines help prevent pneumococcal disease, which is any type of infection caused by Streptococcus pneumoniae bacteria.

There are two kinds of pneumococcal vaccines available in the United States:
 - Pneumococcal conjugate vaccine
 - Pneumococcal polysaccharide vaccine</rdfs:comment>
        <rdfs:label xml:lang="en">Pneumococcus Vaccination</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-010000 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-010000">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Medical-Condition"/>
        <rdfs:comment xml:lang="en">The circulatory system is a vast network of organs and vessels that is responsible for the flow of blood, nutrients, hormones, oxygen and other gases to and from cells.

More information available at https://www.livescience.com/22486-circulatory-system.html</rdfs:comment>
        <rdfs:label xml:lang="en">Circulatory System Disease</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-011000 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-011000">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-010000"/>
        <acronym>CAD</acronym>
        <aka>Ischemic heart disease, IHD</aka>
        <rdfs:comment xml:lang="en">Coronary artery disease (CAD), also known as ischemic heart disease (IHD), is the most common of the cardiovascular diseases. Types include stable angina, unstable angina, myocardial infarction, and sudden cardiac death.

More information available at https://en.wikipedia.org/wiki/Coronary_artery_disease</rdfs:comment>
        <rdfs:label xml:lang="en">Coronary Artery Disease</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-011100 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-011100">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-011000"/>
        <acronym>MI</acronym>
        <aka>Heart attack</aka>
        <rdfs:comment xml:lang="en">Myocardial infarction (MI), commonly known as a heart attack, occurs when blood flow decreases or stops to a part of the heart, causing damage to the heart muscle.

More information available at https://en.wikipedia.org/wiki/Myocardial_infarction</rdfs:comment>
        <rdfs:label xml:lang="en">Myocardial Infarction</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-011200 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-011200">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-011000"/>
        <aka>Angina pectoris</aka>
        <rdfs:comment xml:lang="en">Angina, also known as angina pectoris, is chest pain or pressure, usually due to not enough blood flow to the heart muscle.

https://en.wikipedia.org/wiki/Angina</rdfs:comment>
        <rdfs:label xml:lang="en">Angina</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-011210 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-011210">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-011200"/>
        <rdfs:comment xml:lang="en">Also known as &apos;effort angina&apos;, this refers to the classic type of angina related to myocardial ischemia.</rdfs:comment>
        <rdfs:label xml:lang="en">Stable Angina</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-011220 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-011220">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-011200"/>
        <acronym>UA</acronym>
        <rdfs:comment xml:lang="en">Unstable angina (UA) is a type of angina pectoris that is irregular. It is also classified as a type of acute coronary syndrome (ACS).

More information available at https://en.wikipedia.org/wiki/Unstable_angina</rdfs:comment>
        <rdfs:label xml:lang="en">Unstable Angina</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-020000 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-020000">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Medical-Condition"/>
        <rdfs:comment xml:lang="en">Digestive diseases are disorders of the digestive tract, which is sometimes called the gastrointestinal (GI) tract.

More information available at https://medlineplus.gov/ency/article/007447.htm</rdfs:comment>
        <rdfs:label xml:lang="en">Digestive System Disease</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-021000 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-021000">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-020000"/>
        <rdfs:comment xml:lang="en">Liver disease is any disturbance of liver function that causes illness.</rdfs:comment>
        <rdfs:label xml:lang="en">Liver Disease</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-021100 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-021100">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-021000"/>
        <acronym>AIH</acronym>
        <aka>Lupoid Hepatitis</aka>
        <rdfs:comment xml:lang="en">Autoimmune Hepatitis (AIH), formerly called Lupoid Hepatitis, is a chronic, autoimmune disease of the liver that occurs when the body&apos;s immune system attacks liver cells causing the liver to be inflamed.</rdfs:comment>
        <rdfs:label xml:lang="en">Autoimmune Hepatitis</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-021200 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-021200">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-021000"/>
        <acronym>PBC</acronym>
        <aka>Primary Biliary Cirrhosis</aka>
        <rdfs:comment xml:lang="en">Primary biliary cholangitis (PBC), formerly known as Primary Biliary Cirrhosis, is a chronic disease of the liver, presumably autoimmune in nature, that leads to progressive cholestasis and often end-stage liver disease.</rdfs:comment>
        <rdfs:label xml:lang="en">Primary Biliary Cholangitis</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-022000 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-022000">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-020000"/>
        <aka>sprue, nontropical sprue, gluten-sensitive enteropathy</aka>
        <rdfs:comment xml:lang="en">Celiac disease is a digestive disorder caused by an abnormal immune reaction to gluten.

Celiac disease is also known as: sprue, nontropical sprue, gluten-sensitive enteropathy

More information available at https://www.healthline.com/health/celiac-disease-sprue</rdfs:comment>
        <rdfs:label xml:lang="en">Celiac Disease</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-023000 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-023000">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-020000"/>
        <rdfs:comment xml:lang="en">Autoimmune atrophic gastritis is a chronic inflammatory disease in which the immune system mistakenly destroys a special type of cell (parietal cells) in the stomach.

More information available at https://rarediseases.info.nih.gov/diseases/10310/autoimmune-atrophic-gastritis</rdfs:comment>
        <rdfs:label xml:lang="en">Autoimmune Gastritis</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-030000 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-030000">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Medical-Condition"/>
        <rdfs:comment xml:lang="en">Endocrine diseases are disorders of the endocrine system. 

The endocrine system is a chemical messenger system consisting of hormones, the group of glands of an organism that secrete those hormones directly into the circulatory system to regulate the function of distant target organs, and the feedback loops which modulate hormone release so that homeostasis is maintained. 

In humans, the major endocrine glands are the thyroid gland and the adrenal glands.</rdfs:comment>
        <rdfs:label xml:lang="en">Endocrine System Disease</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-031000 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-031000">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-030000"/>
        <rdfs:comment xml:lang="en">Thyroid dysfunction due to autoimmunity.</rdfs:comment>
        <rdfs:label xml:lang="en">Autoimmune Thyroid Disease</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-032000 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-032000">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-030000"/>
        <rdfs:comment xml:lang="en">Adrenal insufficiency due to autoimmunity.</rdfs:comment>
        <rdfs:label xml:lang="en">Autoimmune Adrenal Disease</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-033000 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-033000">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-030000"/>
        <rdfs:comment xml:lang="en">Ovarian failure due to autoimmunity.</rdfs:comment>
        <rdfs:label xml:lang="en">Autoimmune Ovarian Disease</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-040000 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-040000">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Medical-Condition"/>
        <rdfs:comment xml:lang="en">Any medical condition, that affects the skin or the structures associated with the skin such as hair, nails, and related muscle and glands.</rdfs:comment>
        <rdfs:label xml:lang="en">Cutaneous Disease</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-041000 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-041000">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-040000"/>
        <rdfs:comment xml:lang="en">A maculopapular rash is a type of rash characterized by a flat, red area on the skin that is covered with small confluent bumps.

More information available at https://en.wikipedia.org/wiki/Maculopapular_rash</rdfs:comment>
        <rdfs:label xml:lang="en">Maculopapular Rash</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-042000 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-042000">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-040000"/>
        <rdfs:comment xml:lang="en">A descriptive term for a class of skin lesion presenting redness (erythema) in a ring form (anulare).</rdfs:comment>
        <rdfs:label xml:lang="en">Annular Rash</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-043000 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-043000">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-040000"/>
        <acronym>EM</acronym>
        <rdfs:comment xml:lang="en">Erythema multiforme (EM) is a skin condition of unknown cause; it is a type of erythema possibly mediated by deposition of immune complexes (mostly IgM-bound complexes) in the superficial microvasculature of the skin and oral mucous membrane that usually follows an infection or drug exposure.

More information available at https://en.wikipedia.org/wiki/Erythema_multiforme</rdfs:comment>
        <rdfs:label xml:lang="en">Erythema multiform</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-044000 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-044000">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-040000"/>
        <aka>Hives</aka>
        <rdfs:comment xml:lang="en">Hives, also known as Urticaria, is a kind of skin rash with red, raised, itchy bumps.

More information available at https://en.wikipedia.org/wiki/Hives</rdfs:comment>
        <rdfs:label xml:lang="en">Urticarial Rash</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-045000 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-045000">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-040000"/>
        <rdfs:comment xml:lang="en">Palpable purpura is a condition where purpura, which constitutes visible non-blanching hemorrhages, are raised and able to be touched or felt upon palpation.

More information available at https://en.wikipedia.org/wiki/Palpable_purpura</rdfs:comment>
        <rdfs:label xml:lang="en">Palpable Purpura</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-046000 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-046000">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-040000"/>
        <rdfs:comment xml:lang="en">Vasculitic Ulcer is a collection of Inflammatory Conditions that affect the integrity of micro vessels in the human body.</rdfs:comment>
        <rdfs:label xml:lang="en">Vasculitic Ulcer</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-047000 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-047000">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-040000"/>
        <rdfs:comment xml:lang="en">Photosensitivity is an extreme sensitivity to ultraviolet (UV) rays from the sun and other light sources.

More information available at https://www.healthline.com/health/photosensitivity</rdfs:comment>
        <rdfs:label xml:lang="en">Photosensitivity</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-050000 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-050000">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Medical-Condition"/>
        <rdfs:comment xml:lang="en">Lymphatic System Disease (aka Lymphadenopathy)

Lymphadenopathy or adenopathy is disease of the lymph nodes, in which they are abnormal in size, number, or consistency.</rdfs:comment>
        <rdfs:label xml:lang="en">Lymphatic System Disease</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-051000 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-051000">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-050000"/>
        <rdfs:comment xml:lang="en">Regional lymphadenopathy involves enlargement of a single node or multiple contiguous nodal regions.

More information available at https://www.medscape.com/answers/956340-53134/what-is-regional-lymphadenopathy</rdfs:comment>
        <rdfs:label xml:lang="en">Regional Lymphadenopathy</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-051100 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-051100">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-051000"/>
        <rdfs:comment xml:lang="en">Cervical lymphadenopathy refers to lymphadenopathy of the cervical lymph nodes (the glands in the neck)

More information available at https://en.wikipedia.org/wiki/Cervical_lymphadenopathy</rdfs:comment>
        <rdfs:label xml:lang="en">Cervical Lymphadenopathy</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-051200 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-051200">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-051000"/>
        <rdfs:label xml:lang="en">Other Region Lymphadenopathy</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052000 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052000">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-050000"/>
        <rdfs:comment xml:lang="en">Lymph nodes or &apos;glands&apos; or nodes or lymphoid tissue are nodular bodies located throughout the body but clustering in certain areas such as the armpit, back of the neck and the groin. They are part of the lymphatic system.

More information available at https://en.wikipedia.org/wiki/Generalized_lymphadenopathy</rdfs:comment>
        <rdfs:label xml:lang="en">Generalized Lymphadenopathy</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052100 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052100">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052000"/>
        <rdfs:label xml:lang="en">Generalized Lymphadenopathy non-associated with Lymphoma</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052200 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052200">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052000"/>
        <rdfs:comment xml:lang="en">Lymphoma is a group of blood cancers that develop from lymphocytes (a type of white blood cell).

A lymphocyte is one of the subtypes of white blood cell in a vertebrate&apos;s immune system. 

Lymphocytes include natural killer cells (which function in cell-mediated, cytotoxic innate immunity), T cells (for cell-mediated, cytotoxic adaptive immunity), and B cells (for humoral, antibody-driven adaptive immunity).</rdfs:comment>
        <rdfs:label xml:lang="en">Lymphoma</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052300 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052300">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052200"/>
        <acronym>HL</acronym>
        <rdfs:comment xml:lang="en">Hodgkin&apos;s lymphoma (HL) is a type of lymphoma in which cancer originates from a specific type of white blood cells called lymphocytes.

A lymphocyte is one of the subtypes of white blood cell in a vertebrate&apos;s immune system. 

Lymphocytes include natural killer cells (which function in cell-mediated, cytotoxic innate immunity), T cells (for cell-mediated, cytotoxic adaptive immunity), and B cells (for humoral, antibody-driven adaptive immunity).</rdfs:comment>
        <rdfs:label xml:lang="en">Hodgkin&apos;s Lymphoma</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052400 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052400">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052200"/>
        <acronym>NHL</acronym>
        <rdfs:comment xml:lang="en">Non-Hodgkin lymphoma (NHL) is a group of blood cancers that includes all types of lymphoma except Hodgkin&apos;s lymphomas.</rdfs:comment>
        <rdfs:label xml:lang="en">Non-Hodgkin lymphoma (NHL)</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052500 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052500">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052400"/>
        <acronym>MBCN</acronym>
        <rdfs:comment xml:lang="en">B-cell lymphoproliferative disorders are a clonal expansion of the various stages of  B lymphocytes in bone marrow, blood or other tissues.

Mature B cell neoplasms comprise over 90% of lymphoid neoplasms worldwide and there are 4% of nw cancers each year.</rdfs:comment>
        <rdfs:label xml:lang="en">Mature B-cell neoplasms</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052510 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052510">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052500"/>
        <rdfs:comment xml:lang="en">Marginal zone lymphomas are those that develop in the marginal zone or edge of the lymphoid tissue where B-cells are located.

All marginal zone lymphomas are low-grade B-cell Non-Hodgkins Lymphomas (NHL).</rdfs:comment>
        <rdfs:label xml:lang="en">B-cell Marginal Zone Lymphoma</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052511 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052511">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052510"/>
        <aka>MALT lymphoma</aka>
        <rdfs:comment xml:lang="en">MALT lymphoma is a form of lymphoma involving the mucosa-associated lymphoid tissue (MALT), frequently of the stomach, but virtually any mucosal site can be afflicted.

It is a cancer originating from B cells in the marginal zone of the MALT, and is also called extranodal marginal zone B cell lymphoma.</rdfs:comment>
        <rdfs:label xml:lang="en">B-cell Mucosa-associated Lymphoid Tissue (MALT) Lymphoma</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052512 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052512">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052510"/>
        <acronym>NMZL</acronym>
        <rdfs:comment xml:lang="en">Nodal marginal zone B cell lymphoma (NMZL) is an uncommon form of marginal-zone lymphoma that can produce colonization of the follicles in the lymph node.</rdfs:comment>
        <rdfs:label xml:lang="en">B-cell Nodal Marginal Zone Lymphoma</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052513 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052513">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052510"/>
        <acronym>SMZL</acronym>
        <rdfs:comment xml:lang="en">Splenic marginal zone lymphoma (SMZL) is a type of cancer (specifically a lymphoma) made up of B-cells that replace the normal architecture of the white pulp of the spleen.</rdfs:comment>
        <rdfs:label xml:lang="en">B-cell Splenic Marginal Zone Lymphoma</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052521 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052521">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052500"/>
        <rdfs:comment xml:lang="en">Small lymphocytic lymphoma (SLL) is a non-Hodgkin lymphoma affecting the B-lymphocytes of the immune system.

Chronic lymphocytic leukemia (CLL) is a type of cancer in which the bone marrow makes too many lymphocytes (a type of white blood cell).</rdfs:comment>
        <rdfs:label xml:lang="en">B-cell Small Lymphocytic Lymphoma / Chronic lymphocytic leukemia (CLL)</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052522 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052522">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052500"/>
        <acronym>DLBCL</acronym>
        <rdfs:comment xml:lang="en">Diffuse Large B-Cell Lymphoma is an aggressive lymphoma that can arise in lymph nodes or outside of the lymphatic system.</rdfs:comment>
        <rdfs:label xml:lang="en">Diffuse Large B-cell Lymphoma</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052523 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052523">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052500"/>
        <acronym>MCL</acronym>
        <rdfs:comment xml:lang="en">Mantle cell lymphoma is a B-cell non-Hodgkin lymphoma (NHL).</rdfs:comment>
        <rdfs:label xml:lang="en">Mantle B-cell Lymphoma</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052524 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052524">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052500"/>
        <acronym>THRLBCL</acronym>
        <rdfs:comment xml:lang="en">T-cell/histiocyte-rich large B cell lymphoma (THRLBCL) is a distinct entity of aggressive lymphoma</rdfs:comment>
        <rdfs:label xml:lang="en">T-Cell/Histiocyte-Rich B-Cell Lymphoma</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052525 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052525">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052500"/>
        <aka>FO B cells lymphoma</aka>
        <rdfs:comment xml:lang="en">Follicular lymphoma is a B-cell lymphoma characterised by tumour cells that appear in a circular, or clump-like, pattern under the microscope.

Follicular B cells (FO B cells) are a type of B cell that reside in primary and secondary lymphoid follicles (containing germinal centers) of secondary and tertiary lymphoid organs, including spleen and lymph nodes.</rdfs:comment>
        <rdfs:label xml:lang="en">Follicular B-cell Lymphoma</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052526 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052526">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052500"/>
        <rdfs:comment xml:lang="en">Burkitt lymphoma is a cancer of the lymphatic system, particularly B lymphocytes found in the germinal center.</rdfs:comment>
        <rdfs:label xml:lang="en">Burkitt Lymphoma</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052527 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052527">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052500"/>
        <acronym>ALCL</acronym>
        <rdfs:comment xml:lang="en">Anaplastic large-cell lymphoma (ALCL) is a type of non-Hodgkin lymphoma involving aberrant T cells or null lymphocytes.</rdfs:comment>
        <rdfs:label xml:lang="en">Anaplastic large cell lymphoma</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052529 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052529">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052500"/>
        <acronym>MGUS</acronym>
        <rdfs:comment xml:lang="en">Monoclonal gammopathy of undetermined significance (MGUS) is a condition in which a myeloma protein, an abnormal antibody, is found in the blood during standard laboratory blood tests.</rdfs:comment>
        <rdfs:label xml:lang="en">Monoclonal gammopathy of undetermined significance</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052531 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052531">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052500"/>
        <acronym>LPL</acronym>
        <rdfs:comment xml:lang="en">Lymphoplasmacytic lymphoma is a low grade small B-cell non-Hodgkin lymphoma that has a monoclonal B-cell and a monoclonal plasma cell component.</rdfs:comment>
        <rdfs:label xml:lang="en">Lymphoplasmacytic lymphoma</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052532 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052532">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052500"/>
        <acronym>EBV‚Äìpositive LBCL</acronym>
        <rdfs:comment xml:lang="en">Epstein-Barr virus (EBV)-positive Large B-cell lymphoma (LBCL)</rdfs:comment>
        <rdfs:label xml:lang="en">EBV‚Äìpositive Large B-cell Lymphoma</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052590 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052590">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052500"/>
        <acronym>MM</acronym>
        <rdfs:comment xml:lang="en">Multiple myeloma (MM), also known as plasma cell myeloma, is a cancer of plasma cells, a type of white blood cell normally responsible for producing antibodies.</rdfs:comment>
        <rdfs:label xml:lang="en">Multiple Myeloma</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052591 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052591">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052500"/>
        <rdfs:label xml:lang="en">Other mature B-cell neoplasms</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052600 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052600">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052400"/>
        <rdfs:comment xml:lang="en">The mature or peripheral T-cell neoplasms are a biologically and clinically heterogeneous group of rare disorders that result from clonal proliferation of mature post-thymic lymphocytes.

Natural killer (NK) cells are closely related to T cells and neoplasms derived from these are therefore considered within the same group.

More information available at http://exp-oncology.com.ua/article/4377/mature-t-and-nk-cell-neoplasms</rdfs:comment>
        <rdfs:label xml:lang="en">Mature T- and NK- cell neoplasms</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052700 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052700">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052400"/>
        <rdfs:comment xml:lang="en">Dendritic and histiocytic cell neoplasms are rare malignancies that make up less than 1% of all neoplasms arising in lymph nodes or soft tissues.</rdfs:comment>
        <rdfs:label xml:lang="en">Histiocytic and Dendritic cell neoplasms</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052800 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052800">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-052400"/>
        <acronym>PTLD</acronym>
        <rdfs:comment xml:lang="en">Post-transplant lymphoproliferative disorder (PTLD) is the name given to a B-cell proliferation due to therapeutic immunosuppression after organ transplantation.</rdfs:comment>
        <rdfs:label xml:lang="en">Posttransplant lymphoproliferative disorder</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-060000 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-060000">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Medical-Condition"/>
        <aka>Myositis</aka>
        <rdfs:comment xml:lang="en">Muscular System Disease (aka Myositis) is inflammation or swelling of the muscles.</rdfs:comment>
        <rdfs:label xml:lang="en">Muscular System Disease</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-061000 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-061000">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-060000"/>
        <acronym>IIM</acronym>
        <rdfs:comment xml:lang="en">Idiopathic inflammatory myopathy is a group of disorders characterized by inflammation of the muscles used for movement (skeletal muscles).

More information available at https://ghr.nlm.nih.gov/condition/idiopathic-inflammatory-myopathy</rdfs:comment>
        <rdfs:label xml:lang="en">Idiopathic Inflammatory Myopathy</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-062000 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-062000">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-060000"/>
        <aka>Inclusion-Body Myositis, IBM</aka>
        <rdfs:comment xml:lang="en">Inclusion-Body Myositis (IBM) is muscle disease characterized by inflammation and degeneration of the muscles leading to muscle weakness.

More information available at https://www.mda.org/disease/inclusion-body-myositis</rdfs:comment>
        <rdfs:label xml:lang="en">Inclusion Body Myositis (IBM) documented with Biopsy</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-070000 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-070000">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#Medical-Condition"/>
        <aka>Neurological disorders</aka>
        <rdfs:comment xml:lang="en">Nervous system diseases, also known as nervous system or neurological disorders, refers to a small class of medical conditions affecting the nervous system.

More information available at https://en.wikipedia.org/wiki/Nervous_system_disease</rdfs:comment>
        <rdfs:label xml:lang="en">Nervous System Disease</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-071000 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-071000">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-070000"/>
        <acronym>CNS</acronym>
        <rdfs:comment xml:lang="en">Central nervous system diseases, also known as central nervous system disorders, are a group of neurological disorders that affect the structure or function of the brain or spinal cord, which collectively form the central nervous system (CNS).</rdfs:comment>
        <rdfs:label xml:lang="en">Central Nervous System Disease</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-071100 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-071100">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-071000"/>
        <rdfs:label xml:lang="en">Multiple Sclerosis (MS) like</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-071200 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-071200">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-071000"/>
        <acronym>NMO</acronym>
        <aka>Devic&apos;s disease, Devic&apos;s syndrome</aka>
        <rdfs:comment xml:lang="en">Neuromyelitis optica (NMO), also known as Devic&apos;s disease or Devic&apos;s syndrome, is a heterogeneous condition consisting of the inflammation and demyelination of the optic nerve (optic neuritis) and the spinal cord (myelitis).

More information available at https://en.wikipedia.org/wiki/Neuromyelitis_optica</rdfs:comment>
        <rdfs:label xml:lang="en">Neuromyelitis Optica</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-071300 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-071300">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-071000"/>
        <acronym>TM</acronym>
        <rdfs:comment xml:lang="en">Transverse myelitis (TM) is a rare neurological condition in which the spinal cord is inflamed.

More information available at https://en.wikipedia.org/wiki/Transverse_myelitis</rdfs:comment>
        <rdfs:label xml:lang="en">Transverse Myelitis</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-071400 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-071400">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-071000"/>
        <aka>Central nervous system vasculitis, CNS vasculitis</aka>
        <rdfs:comment xml:lang="en">Central nervous system (CNS) vasculitis is inflammation of blood vessel walls in the brain or spine. 

More information available at https://my.clevelandclinic.org/health/diseases/13205-central-nervous-system-vasculitis</rdfs:comment>
        <rdfs:label xml:lang="en">Cerebral Vasculitis</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-071500 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-071500">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-071000"/>
        <rdfs:label xml:lang="en">Lymphocytic Meningitis</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-071600 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-071600">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-071000"/>
        <aka>Cerebrovascular accident, CVA</aka>
        <rdfs:comment xml:lang="en">Cerebrovascular accident (CVA) is the medical term for a stroke.

A stroke is when blood flow to a part of your brain is stopped either by a blockage or the rupture of a blood vessel.

More information available at https://www.healthline.com/health/cerebrovascular-accident</rdfs:comment>
        <rdfs:label xml:lang="en">Cerebral Stroke</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-072000 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-072000">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-070000"/>
        <rdfs:comment xml:lang="en">Diseases of the peripheral nerves external to the brain and spinal cord, which includes diseases of the nerve roots, ganglia, plexi, autonomic nerves, sensory nerves, and motor nerves.</rdfs:comment>
        <rdfs:label xml:lang="en">Peripheral Nervous System Disease</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-072100 -->

