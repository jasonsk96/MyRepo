        <quest-Outcome>Number and/or Assessment</quest-Outcome>
        <quest-Score-Normal-Range>0</quest-Score-Normal-Range>
        <rdfs:label xml:lang="en">Oral Sicca Visual Analogue Scale (VAS) score</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-082 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-082">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#QUEST-080"/>
