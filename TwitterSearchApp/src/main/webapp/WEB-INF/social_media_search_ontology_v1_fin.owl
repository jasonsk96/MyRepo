        <rdfs:comment xml:lang="en">Glomerulopathy is a set of diseases affecting the glomeruli of the nephron.</rdfs:comment>
        <rdfs:label xml:lang="en">Glomerulopathy</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-081110 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ntua/iccs/harmonicss/terminology/vocabulary#COND-081110">
